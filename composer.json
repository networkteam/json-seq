{
    "name": "networkteam/json-seq",
    "description": "JSON Text Sequences (RFC7464) encoder and decoder",
    "type": "library",
    "license": "MIT",
    "keywords": ["json"],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": { "Networkteam\\JsonSeq\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Networkteam\\JsonSeq\\Tests\\": "tests/" }
    },
     "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    }
}
